{
	"manifest_version": 2,
	"name": "Shelter+",
	"description": "Shelter",
	"version": "1.0.0",
	"options_page": "options/main/index.html",
	"icons": {
		"128": "imgs/logo/logo.png"
	},
	"browser_action": {
		"default_popup": "redirect/redirect.html",
		"default_icon": "imgs/logo/logo.png"
	},
	"content_scripts": [],
	"permissions": [
		"system.network",
		"proxy",
		"storage",
		"cookies",
		"activeTab",
		"*://*/*",
		"tabs",
		"webRequest",
		"webRequestBlocking",
		"webNavigation",
		"unlimitedStorage",
		"<all_urls>"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
	"web_accessible_resources": [
		"*.json"
	]
}